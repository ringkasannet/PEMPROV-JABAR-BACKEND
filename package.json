{
    "name": "pemprov-jabar-backend",
    "version": "1.0.0",
    "description": "",
    "main": "/dist/index.js",
    "scripts": {
        "start-old": "cross-env NODE_ENV=production pm2 start src/index.js",
        "dev-old": "cross-env NODE_ENV=development nodemon src/index.js",
        "test-old": "echo \"Error: no test specified\" && exit 1",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "build": "npm run build-ts",
        "serve": "node dist/index.js",
        "test": "jest --coverage=false --verbose",
        "watch": "cross-env NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-node": "nodemon dist/index.js",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "eslint src/*.ts",
        "lint-fix": "yarn lint --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/index.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "typecheck": "tsc --noEmit",
        "run-prettier": "prettier src/*.ts",
        "validate-prettiness": "yarn run-prettier -c",
        "make-prettier": "yarn run-prettier --write"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@google/generative-ai": "^0.17.1",
        "@pinecone-database/pinecone": "^2.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "https": "^1.0.0",
        "mongodb": "^6.5.0",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.38.3",
        "openai-gpt-token-counter": "^1.1.1"
    },
    "devDependencies": {
        "@swc/core": "^1.7.18",
        "@swc/jest": "^0.2.36",
        "@types/cors": "^2.8.17",
        "@types/express": "4.17.21",
        "@types/http-errors": "1.8.2",
        "@types/jest": "^26.0.14",
        "@types/morgan": "1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^22.5.0",
        "@types/supertest": "2.0.16",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "autoprefixer": "^10.4.20",
        "concurrently": "6.5.1",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.10.0",
        "eslint-plugin-import": "2.29.1",
        "jest": "26.6.3",
        "nodemon": "2.0.22",
        "postcss": "^8.4.42",
        "prettier": "2.8.8",
        "supertest": "6.3.3",
        "tailwindcss": "^3.4.10",
        "ts-jest": "26.5.6",
        "typescript": "4.9.5"
    }
}
